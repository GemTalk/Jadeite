create: viewName
	| testsActive |
	testsActive := Smalltalk at: #JadeiteTestsActive ifAbsent: [^super create: viewName].
	^testsActive
		ifTrue: [(Smalltalk at: #TestChoicePrompter) basicNew]
		ifFalse: [super create: viewName]! !
