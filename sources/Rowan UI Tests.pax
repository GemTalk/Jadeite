	| testsActive |
	testsActive := Smalltalk at: #JadeiteTestsActive ifAbsent: [^self basicNew initialize].
	^testsActive
		ifTrue: [(Smalltalk at: #TestMessageBox) basicNew initialize]
		ifFalse: [self basicNew initialize] ! !
